{
  "editor.fontFamily": "Consolas",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.wordWrap": "off",

  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.matchBrackets": "always",
  "editor.renderWhitespace": "boundary",
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "line",
  "editor.occurrencesHighlight": "singleFile",
  "editor.overviewRulerBorder": false,

  "editor.minimap.enabled": true,
  "editor.smoothScrolling": false,
  "workbench.list.smoothScrolling": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "auto",

  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.hover.enabled": true,
  "editor.parameterHints.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.suggestOnTriggerCharacters": true,
  "editor.suggest.preview": true,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.snippetsPreventQuickSuggestions": true,

  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },

  "editor.lineNumbers": "on",
  "editor.folding": true,
  "editor.foldingStrategy": "indentation",
  "breadcrumbs.enabled": true,
  "editor.glyphMargin": true,

  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": true,

  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.eol": "\n",

  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/.turbo": true,
    "**/.cache": true,
    "**/__pycache__": true,
    "**/.venv": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/.turbo": true,
    "**/.cache": true,
    "**/__pycache__": true,
    "**/.venv": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.next/**": true,
    "**/.turbo/**": true,
    "**/.cache/**": true,
    "**/__pycache__/**": true,
    "**/.venv/**": true,
    "**/out/**": true,
    "**/bin/**": true,
    "**/.vscode/**": true
  },

  "search.followSymlinks": false,
  "search.quickOpen.includeSymbols": true,

  "editor.autoIndent": "advanced",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.autoClosingQuotes": "languageDefined",
  "editor.snippetSuggestions": "inline",
  "typescript.suggest.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "javascript.suggest.enabled": true,
  "emmet.triggerExpansionOnTab": true,

  "workbench.iconTheme": "vs-minimal",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": true,
  "workbench.quickOpen.preserveInput": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 12,
  "workbench.editor.focusRecentEditorAfterClose": false,

  "git.autofetch": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.suggestSmartCommit": false,
  "git.openRepositoryInParentFolders": "always",
  "git.pruneOnFetch": true,

  "terminal.integrated.gpuAcceleration": "auto",
  "terminal.integrated.fontFamily": "Consolas",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.scrollback": 12000,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",

  "telemetry.enableCrashReporter": false,
  "telemetry.feedback.enabled": false,
  "extensions.ignoreRecommendations": true,
  "chat.agent.enabled": false,
  "markdown.preview.fontSize": 14,
  "workbench.colorCustomizations": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#C3CCDF",
    "editorCursor.foreground": "#63C3B8",

    "editor.lineHighlightBackground": "#292E42",
    "editor.selectionBackground": "#2b3e5e",
    "editor.inactiveSelectionBackground": "#2b3e5e80",
    "editor.selectionHighlightBackground": "#364261",
    "editor.findMatchBackground": "#D64A44A4",
    "editor.findMatchBorder": "#D64A64",
    "editor.findMatchHighlightBackground": "#8BCFEA33",
    "editor.findRangeHighlightBackground": "#3a406166",

    "editorGutter.background": "#1a1b26",
    "editorGutter.foldingControlForeground": "#8C8AF0",
    "editorLineNumber.foreground": "#8C8AF0",
    "editor.activeLineNumberForeground": "#B879BE",

    "editorError.foreground": "#FF6E6E",
    "editorWarning.foreground": "#FFD11A",
    "editorInfo.foreground": "#6FD6F5",
    "editorHint.foreground": "#A6D27A",

    "editorBracketMatch.background": "#00000000",
    "editorBracketMatch.border": "#8C8AF0",

    "sideBar.background": "#1a1b26",
    "sideBar.foreground": "#88C3EA",
    "list.hoverBackground": "#24283b",
    "list.activeSelectionBackground": "#2b3e5e",
    "list.activeSelectionForeground": "#88C3EA",
    "list.inactiveSelectionBackground": "#24283b",

    "activityBar.background": "#1a1b26",
    "activityBar.foreground": "#88C3EA",
    "activityBarBadge.background": "#8C8AF0",
    "activityBarBadge.foreground": "#1a1b26",
    "statusBar.background": "#24283b",
    "statusBar.foreground": "#FFB107",

    "terminal.background": "#1a1b26",
    "terminal.foreground": "#88C3EA",
    "terminalCursor.foreground": "#63C3B8",
    "terminal.ansiBlack": "#1b1c25",
    "terminal.ansiRed": "#FF4136",
    "terminal.ansiGreen": "#2ECC40",
    "terminal.ansiYellow": "#FFB107",
    "terminal.ansiBlue": "#1E90FF",
    "terminal.ansiMagenta": "#B879BE",
    "terminal.ansiCyan": "#00BCD4",
    "terminal.ansiWhite": "#C3CCDF",
    "terminal.ansiBrightBlack": "#414968",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#87D38C",
    "terminal.ansiBrightYellow": "#FFA726",
    "terminal.ansiBrightBlue": "#8CB3F2",
    "terminal.ansiBrightMagenta": "#CAC6FF",
    "terminal.ansiBrightCyan": "#26C6DA",
    "terminal.ansiBrightWhite": "#E6E9F7",

    "titleBar.activeBackground": "#24283b",
    "titleBar.activeForeground": "#88C3EA",
    "breadcrumb.foreground": "#8C8AF0",
    "editorGroupHeader.tabsBackground": "#1a1b26"
  },

  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "comments": "#6A9955",
    "functions": "#1E90FF",
    "keywords": "#B879BE",
    "strings": "#FF4136",
    "numbers": "#2ECC40",
    "variables": "#FFFFFF",
    "types": "#00BCD4",
    "textMateRules": [
      { "scope": "comment", "settings": { "foreground": "#6A9955", "fontStyle": "italic" } },
      { "scope": "comment.block.documentation", "settings": { "foreground": "#8E8E8E" } },

      { "scope": ["entity.name.function","support.function"], "settings": { "foreground": "#1E90FF" } },

      { "scope": "keyword", "settings": { "foreground": "#B879BE", "fontStyle": "bold" } },
      { "scope": "keyword.control", "settings": { "foreground": "#8C8AF0" } },
      { "scope": "keyword.operator", "settings": { "foreground": "#FFFFFF" } },
      { "scope": "keyword.operator.comparison", "settings": { "foreground": "#277DFF" } },

      { "scope": "string", "settings": { "foreground": "#FF4136" } },
      { "scope": "punctuation.definition.template-expression", "settings": { "foreground": "#D72E82" } },
      { "scope": "meta.template.expression", "settings": { "foreground": "#EEF6FF" } },

      { "scope": "constant.numeric", "settings": { "foreground": "#2ECC40" } },
      { "scope": "constant.numeric.negative", "settings": { "foreground": "#FF4136" } },
      { "scope": "constant.language", "settings": { "foreground": "#FF4136" } },

      { "scope": "variable", "settings": { "foreground": "#FFFFFF" } },
      { "scope": "variable.global", "settings": { "foreground": "#008080" } },
      { "scope": "variable.parameter", "settings": { "foreground": "#FF7A00" } },
      { "scope": "variable.other.property", "settings": { "foreground": "#BEBEBE" } },
      { "scope": "variable.language", "settings": { "foreground": "#FFB107" } },

      { "scope": ["storage.type","entity.name.class","storage.modifier.vb"], "settings": { "foreground": "#00BCD4" } },

      { "scope": ["entity.name.tag","support.tag"], "settings": { "foreground": "#FF8686", "fontStyle": "bold" } },
      { "scope": "entity.name.label", "settings": { "foreground": "#FFA726" } },
      { "scope": "entity.other.attribute-name", "settings": { "foreground": "#2ECC40" } },
      { "scope": "support.type.property-name.css", "settings": { "foreground": "#2ECC40" } },
      { "scope": "entity.other.attribute-name.pseudo-class", "settings": { "foreground": "#FFA726" } },
      { "scope": "support.type.property-name.json", "settings": { "foreground": "#FFA726" } },

      { "scope": "meta.function.decorator.python", "settings": { "foreground": "#FFA726" } },
      { "scope": "meta.preprocessor", "settings": { "foreground": "#F4511E" } },

      { "scope": "punctuation", "settings": { "foreground": "#FFFFFF" } },
      { "scope": "punctuation.separator", "settings": { "foreground": "#BABABA" } },
      { "scope": "markup.heading", "settings": { "foreground": "#FFB107" } },
      { "scope": "markup.underline.link", "settings": { "foreground": "#1E90FF" } },
      { "scope": "markup.italic", "settings": { "foreground": "#FFC107", "fontStyle": "italic" } }
    ]
  }
}
